{
  "name": "root",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Bureau of Global Public Affairs, U.S. Department of State"
  },
  "scripts": {
    "analyze": "npm run analyze:articleEmbed && npm run analyze:articleFeed",
    "analyze:articleEmbed": "webpack --env articleEmbed --config config/webpack.analyze.js --mode production",
    "analyze:articleFeed": "webpack --env articleFeed --config config/webpack.analyze.js --mode production",
    "build": "npm run build:articleEmbed && npm run build:articleFeed",
    "build:articleEmbed": "webpack --env articleEmbed --config config/webpack.config.js --mode production",
    "build:articleFeed": "webpack --env articleFeed --config config/webpack.config.js --mode production",
    "dev": "npm run dev:articleEmbed & npm run dev:articleFeed & npm run example",
    "dev:articleEmbed": "webpack --env articleEmbed --watch --config config/webpack.config.js --mode development",
    "dev:articleFeed": "webpack --env articleFeed --watch --config config/webpack.config.js --mode development",
    "example": "webpack-dev-server --env example --config config/webpack.config.js --mode development",
    "lint": "eslint --ext .js ./"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@gpa-lab/eslint-config": "^1.2.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-css-modules": "^5.2.6",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.5.3",
    "dotenv-webpack": "^1.8.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "html-webpack-plugin": "^4.3.0",
    "lerna": "^3.22.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "postcss-nested": "^4.2.1",
    "postcss-scss": "^2.1.1",
    "sass-loader": "^8.0.2",
    "svg-inline-loader": "^0.8.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
