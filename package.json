{
  "name": "@gpa-lab/cdp-modules",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Bureau of Global Public Affairs, U.S. Department of State"
  },
  "scripts": {
    "analyze": "npm run analyze:articleEmbed && npm run analyze:articleFeed",
    "analyze:articleEmbed": "webpack --env articleEmbed --config config/webpack.analyze.js --mode production",
    "analyze:articleFeed": "webpack --env articleFeed --config config/webpack.analyze.js --mode production",
    "build": "npm run build:articleEmbed && npm run build:articleFeed",
    "build:articleEmbed": "webpack --env articleEmbed --config config/webpack.config.js --mode production",
    "build:articleFeed": "webpack --env articleFeed --config config/webpack.config.js --mode production",
    "dev": "npm run dev:articleEmbed & npm run dev:articleFeed & npm run example",
    "dev:articleEmbed": "webpack --env articleEmbed --watch --config config/webpack.config.js --mode development",
    "dev:articleFeed": "webpack --env articleFeed --watch --config config/webpack.config.js --mode development",
    "example": "webpack-dev-server --env example --config config/webpack.config.js --mode development",
    "lint": "eslint --ext .js ./"
  },
  "workspaces": [
    "example",
    "modules/article-embed",
    "modules/article-feed"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@gpa-lab/eslint-config": "^1.6.1",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.15.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "lerna": "^4.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0"
  }
}
